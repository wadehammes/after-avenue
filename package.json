{
  "name": "next-after-avenue",
  "version": "0.0.0",
  "packageManager": "pnpm@10.29.3",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 3005 --webpack",
    "dev:webpack": "NODE_OPTIONS='--inspect' next dev -p 3005 --webpack",
    "build": "next build && make sitemap",
    "build:analyze": "next experimental-analyze && make sitemap",
    "build:analyze:legacy": "ANALYZE=true next build && make sitemap",
    "start": "next start -p 3005",
    "export": "next export",
    "tsc:ci": "tsc --strict",
    "lint": "biome check",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --fix .",
    "test:ci": "jest --passWithNoTests",
    "types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -r -g -X && biome format --write src/contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^17.1.5",
    "@contentful/rich-text-react-renderer": "^16.1.5",
    "@contentful/rich-text-types": "^17.2.4",
    "@faker-js/faker": "^10.2.0",
    "@jest/types": "^30.2.0",
    "@next/third-parties": "^16.1.6",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.90.20",
    "classnames": "^2.5.1",
    "contentful": "^11.10.3",
    "html-react-parser": "^5.2.16",
    "jest-mock": "^30.2.0",
    "next": "^16.1.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^11.1.2",
    "postcss-syntax": "^0.36.2",
    "react": "^19.2.4",
    "react-aria": "^3.46.0",
    "react-dom": "^19.2.4",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.71.1",
    "react-intersection-observer": "^10.0.2",
    "react-player": "^3.3.3",
    "resend": "6.9.2",
    "safe-json-stringify": "^1.2.0",
    "schema-dts": "^1.1.5",
    "tslib": "^2.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.11",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/safe-json-stringify": "^1.1.5",
    "cf-content-types-generator": "^2.16.1",
    "csstype": "^3.1.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "react-is": "^19.2.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack-bundle-analyzer": "^5.2.0"
  }
}
